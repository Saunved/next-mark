{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals", "airbnb", "prettier"],
  "plugins": ["prettier", "react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": ["node_modules", "./"]
        }
      }
    }
}
